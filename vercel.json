{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "rewrites": [
    { "source": "/m", "destination": "/index.html" },
    { "source": "/m/(.*)", "destination": "/index.html" },
    { "source": "/app", "destination": "/index.html" },
    { "source": "/app/(.*)", "destination": "/index.html" },
    { "source": "/((?!static|assets|favicon|manifest|robots).*)", "destination": "/index.html" }
  ]
}
